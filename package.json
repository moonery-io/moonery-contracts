{
  "name": "moonery",
  "description": "Moonery $MNRY - Moonery is a community-driven, no-loss price game and no-loss pool launchpad built on Binance Smart Chain #BSC. We bring the most revolutionary mechanism to the BSC ecosystem.",
  "version": "1.0.0",
  "scripts": {
    "fork": "ganache-cli --fork https://data-seed-prebsc-1-s1.binance.org:8545",
    "compile": "hardhat compile",
    "coverage": "hardhat coverage",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:fix": "npm run lint:js:fix",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ignore-path .gitignore . --fix",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "test": "hardhat test",
    "deploy": "npx hardhat run scripts/deploy.js",
    "deploy:testnet": "npx hardhat run --network testnet scripts/deploy.js",
    "deploy:escrow": "npx hardhat run --network testnet scripts/deploy_escrow.js",
    "deploy:mainnet": "npx hardhat run --network mainnet scripts/deploy.js",
    "deploy:fork": "npx hardhat run --network fork scripts/deploy.js",
    "flattener": "truffle-flattener contracts/Moonery.sol > contracts/FlattenedMoonery.sol",
    "verify:testnet": "npx hardhat verify --libraries scripts/libraries.js  --constructor-args scripts/arguments.js --network testnet 0x9CB19eE44e5c4A6741492916cD397F9e8EfC9D2C",
    "verify:mainnet": "npx hardhat verify --libraries scripts/libraries.js  --constructor-args scripts/arguments.js --network mainnet 0x49363b69f7702a5C33F8F609A694122e2F219D20 --show-stack-traces",
    "verify:sale": "npx hardhat verify --constructor-args scripts/saleArguments.js --network mainnet 0x3D2c7f453cE841CfE8829178c76A54cAa21E3ec8",
    "migrate:testnet": "./node_modules/.bin/truffle migrate --network testnet",
    "gas-report": "env ENABLE_GAS_REPORT=true npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moonery-io/moonery-contracts"
  },
  "author": "Moonery <DEV>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moonery-io/moonery-contracts/issues"
  },
  "homepage": "https://moonery.io/",
  "devDependencies": {
    "@nomiclabs/buidler-truffle5": "^1.3.4",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.2",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^3.4.0",
    "@openzeppelin/contracts4": "npm:@openzeppelin/contracts@^4.0.0",
    "@openzeppelin/docs-utils": "^0.1.0",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/test-helpers": "^0.5.10",
    "@truffle/hdwallet-provider": "^1.2.2",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "chai": "^4.3.4",
    "eslint": "^7.25.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eth-sig-util": "^3.0.1",
    "ethereum-waffle": "^3.3.0",
    "ethereumjs-util": "^7.0.10",
    "ethereumjs-wallet": "^1.0.1",
    "ethers": "^5.1.0",
    "graphviz": "0.0.9",
    "hardhat": "^2.2.1",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-gas-reporter": "^1.0.4",
    "keccak256": "^1.0.2",
    "lodash.startcase": "^4.4.0",
    "lodash.zip": "^4.2.0",
    "merkletreejs": "^0.2.18",
    "micromatch": "^4.0.4",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2",
    "solhint": "^3.3.4",
    "solidity-coverage": "^0.7.16",
    "solidity-docgen": "^0.5.13",
    "truffle": "^5.3.0",
    "truffle-flattener": "^1.5.0",
    "truffle-plugin-verify": "^0.5.7",
    "web3": "^1.3.4",
    "web3-eth-contract": "^1.3.4"
  },
  "dependencies": {
    "@pooltogether/pooltogether-contracts": "^3.3.8"
  }
}
